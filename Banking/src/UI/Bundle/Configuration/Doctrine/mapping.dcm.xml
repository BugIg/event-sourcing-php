<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                   https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="Madkom\ES\Banking\Domain\Account\Account">

        <indexes>
            <index name="account_id_idx" columns="account_id"/>
            <index name="client_id_idx" columns="client_id"/>
        </indexes>

        <unique-constraints>
            <unique-constraint columns="accountID,user_email" name="search_idx" />
        </unique-constraints>


        <field name="accountID" column="account_id" type="string" length="32" unique="true" version="true" />
        <field name="clientID" column="client_id" type="string" length="32" unique="true" />
        <field name=""
        <field name="active" column="active" type="boolean" length="32" unique="true" />

    </entity>

</doctrine-mapping>